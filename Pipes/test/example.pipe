pipe get_flickr

input
arguments $count

with
    find is $count
    images is [item]
apply flickr

output

pipe main

load utils

with url is "" extract feed

apply analyse_content

with
    on is [item]
    assign is [item]
    trans is with count is 5 apply get_flickr
    emit is all
apply loop

output

