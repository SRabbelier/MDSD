pipe get_flickr

	input
	arguments $count
	
	with
	    find is $count
	    images is [item]
	apply flickr

output

pipe main

	load utils
	
	with url is "" extract feed
	
	apply analyse_content
	
	with
	    //on is [item]
	    on_fail is [item]
	    on_failing is "meh"
	    assign is [item]
	    trans is with count is 66 apply get_flickr
	    emit is all
	apply loop

output

pipe main

	with url is "example.com/feed" extract feed
	
	apply analyse_content
	
	with
	    on is [item]
	    assign is [item]
	    trans is with 
	        find is 5
	        images is [item]
	        apply flickr
	    emit is all
	apply oops

output

pipe multi

	$feed1 becomes with url is "example.com/feed1" extract feed
	$feed2 becomes with url is "example.com/feed2" extract feed
	
	$feed3 becomes apply reverse to $feed1
	
	$union becomes apply union to $feed2 and $feed3

outputs $union


native loop arguments $on and $assign and $trans and $emit
native union
native multi
native reverse
native analyse_content
native flickr
	inputs $stream
	arguments $find and $images
outputs $stream
