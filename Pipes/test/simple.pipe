pipe main

inputs $stream1 and $stream2

arguments $foo and $bar

load utils

with url is "http://www.xkcd.com/rss.xml" extract feed

with
    on is [title]
    query is "the"
apply filter to $stream

with
   on     is [description]
   assign is [image/url]
   trans  is with pattern is "src=\"([^\"]+)\"" apply search_pattern
apply loop

output $foo and $bar
