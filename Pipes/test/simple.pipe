pipe main

load utils

feed, using
 "http://www.xkcd.com/rss.xml" as url
, extracted

filter, using
    [title] as on
    "the" as query
, applied 

loop, using
	[description] as on
	[image/url] as assign
	search_pattern, using "src=\"([^\"]+)\"" as pattern
	, as trans 
, applied

native feed
	arguments $url
native filter
	arguments $on and $query
native loop
	arguments $on and $assign and $trans
native search_pattern
	arguments $pattern
output
